block link-section 
	- let sidebar = { 'page 4': 'page4.html','pagina md 1': 'paginamd1.html', 'pagina md 2': 'paginamd2.html'};

aside(itemscope="itemscope" itemtype="http://schema.org/WPSideBar" role="complementary").ed-item.l-25 
	.vertical-menu-toggle.to-l#vertical-menu-toggle(data-content="Filtros")
	nav(itemscope itemtype="http://schema.org/WPSideBar").vertical-menu#vertical-menu 
		ul
